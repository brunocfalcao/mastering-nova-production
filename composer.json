{
    "name": "brunocfalcao/mastering-nova",
    "type": "project",
    "description": "Mastering Nova website",
    "keywords": [
        "framework",
        "laravel",
        "nova",
        "php"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5||^8.0",
        "blade-ui-kit/blade-heroicons": "^0.1.0",
        "blade-ui-kit/blade-icons": "^0.4.0",
        "brunocfalcao/blade-feather-icons": "^0.1.1",
        "davidhsianturi/blade-bootstrap-icons": "^0.4.0",
        "brunocfalcao/flame": "^2.1",
        "brunocfalcao/helpers": "dev-master",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "geoip2/geoip2": "~2.0",
        "gliterd/laravel-backblaze-b2": "^1.2",
        "guzzlehttp/guzzle": "^6.5",
        "intervention/image": "^2.5",
        "larabug/larabug": "^2.3",
        "laravel/framework": "^7.0",
        "laravel/nexmo-notification-channel": "^2.4",
        "laravel/tinker": "^2.0",
        "pragmarx/tracker": "^4.0",
        "predis/predis": "^1.1",
        "protonemedia/laravel-paddle": "^1.1",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/laravel-honeypot": "^2.2",
        "spatie/laravel-medialibrary": "^8.3",
        "thomaswelton/laravel-gravatar": "^1.2",
        "wildbit/swiftmailer-postmark": "^3.1"
    },
    "require-dev": {
        "filp/whoops": "^2.7",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {
                "WebsiteCheckout": "App\\Facades\\WebsiteCheckout"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "check-style": "php-cs-fixer fix --dry-run --diff",
        "fix-style": "php-cs-fixer fix",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
